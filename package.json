{
  "name": "@sudoplatform/sudo-virtual-cards",
  "version": "0.0.1",
  "author": "Anonyome Labs, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudoplatform/sudo-virtual-cards-js.git"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "concurrently \"yarn lint:eslint\" \"yarn lint:prettier\" \"yarn lint:circular\"",
    "lint:eslint": "eslint \"{bin,test,src}/**/*.{js,ts,tsx}\"",
    "lint:prettier": "prettier -l \"{bin,test,src}/**/*.{js,json,jsx,ts,tsx}\"",
    "lint:circular": "yarn madge --circular --extensions ts src test",
    "codegen": "graphql-codegen -c codegen.yml",
    "verify": "yarn audit && yarn lint",
    "unit-test": "jest --verbose test/unit",
    "integration-test": "jest --verbose test/integration",
    "clean": "rm -rf lib docs",
    "build": "yarn codegen && yarn build:transpile && yarn build:docs",
    "build:transpile": "concurrently \"yarn build:transpile:declarations\" \"yarn build:transpile:cjs\" \"yarn build:transpile:esm\"",
    "build:transpile:cjs": "tsc --project tsconfig.commonjs.json",
    "build:transpile:esm": "tsc",
    "build:transpile:declarations": "tsc --project tsconfig.declarations.json",
    "build:docs": "typedoc --readme README.external.md --treatWarningsAsErrors --excludePrivate --out docs src/index.ts"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "cjs",
    "lib",
    "types"
  ],
  "peerDependencies": {
    "@sudoplatform/sudo-api-client": "^12.1.0",
    "@sudoplatform/sudo-common": "^8.1.0",
    "@sudoplatform/sudo-user": "^12.1.0",
    "@sudoplatform/sudo-web-crypto-provider": "^7.0.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "async-mutex": "^0.4.0",
    "aws-appsync": "^4.1.8",
    "fp-ts": "^2.13.1",
    "graphql": "^15.8.0",
    "io-ts": "^2.2.19",
    "lodash": "^4.17.21",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "redux": "^4.2.0",
    "tslib": "^2.4.0"
  },
  "resolutions": {
    "**/apollo-link": "^1.2.14",
    "**/apollo-cache-inmemory": "^1.6.6",
    "**/node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.0",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/typed-document-node": "^2.3.5",
    "@graphql-codegen/typescript": "^2.7.5",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@graphql-typed-document-node/apollo-client-2": "^1.0.0",
    "@sudoplatform/sudo-api-client": "^12.1.0",
    "@sudoplatform/sudo-common": "^8.1.0",
    "@sudoplatform/sudo-entitlements": "^14.0.0",
    "@sudoplatform/sudo-entitlements-admin": "^11.0.0",
    "@sudoplatform/sudo-profiles": "^15.0.0",
    "@sudoplatform/sudo-secure-id-verification": "^6.0.0",
    "@sudoplatform/sudo-user": "^12.1.0",
    "@sudoplatform/sudo-virtual-cards-simulator": "^4.0.1",
    "@sudoplatform/sudo-web-crypto-provider": "^7.1.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.186",
    "@types/luxon": "^3.0.2",
    "@types/node": "^16.11.68",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "apollo-client": "^2.6.10",
    "checkout-sdk-node": "^2.0.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "husky": "^8.0.1",
    "isomorphic-webcrypto": "^2.3.8",
    "jest": "^27.5.1",
    "luxon": "^3.0.4",
    "madge": "^5.0.1",
    "prettier": "^2.6.0",
    "prettier-plugin-organize-imports": "^3.1.1",
    "stripe": "^10.14.0",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-jest": "^27.1.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.17",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0",
    "wait-for-expect": "^3.0.2",
    "websocket": "^1.0.34"
  }
}
